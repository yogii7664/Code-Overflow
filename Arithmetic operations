
#include <stdio.h>

int main() {
int a,b,result;
char op;
printf ("Enter an expression");
scanf ("%d%c%d",&a,&op,&b);
switch (op)
{
    case '+':
    result=a+b;
    break;
    case '-':
    result=a-b;
    break;
    case '*':
    result=a*b;
    break ;
    case '/':
    result=a/b;
    break;
}
printf ("\n Result=%d",result);
    return 0;
}
